post_install()
{
  echo " GroovyUME and the additional UME utilities have been installed"
  echo " in /usr/share/groovyume "
  echo " You must setup your configuration before use! "
  echo " Run GroovyUME with the commandline below then edit the "
  echo " ini file that was created with your monitor settings: "
  echo " groovyume -cc"
}
